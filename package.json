    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "firebase-admin": "^13.3.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.0.200",
    "morgan": "^1.10.0",
    "openai": "^4.20.1",
    "socket.io": "^4.7.2",
